# AWS Environment for E-Commy
  
# Database (update with actual RDS endpoint when deployed)
DATABASE_URL=postgresql://USER:PASSWORD@your-aws-rds-endpoint:5432/ecommydb
  
# Backend settings
PORT=8080  # AWS Elastic Beanstalk default port
JWT_SECRET=your-production-secret-key-replace-me
  
# Frontend URLs (update with actual CloudFront/domain URLs)
CUSTOMER_FRONTEND_URL=https://your-customer-domain.com
ADMIN_FRONTEND_URL=https://your-admin-domain.com
  
# Cloudinary (same as your development setup)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# CORS Settings (space-separated origins, add your actual domains)
CORS_ORIGIN="https://your-customer-domain.com https://your-admin-domain.com" 